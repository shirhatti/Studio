{
  "name": "studio",
  "version": "1.0.0",
  "description": "Diagnostics Studio",
  "author": "Sourabh Shirhatti",
  "main": "main.js",
  "scripts": {
    "build": "yarn run tsc",
    "protoc": "yarn run grpc_tools_node_protoc --proto_path=\".\\protos\\\" --plugin=\"protoc-gen-grpc=.\\node_modules\\.bin\\grpc_tools_node_protoc_plugin.cmd\" --grpc_out=\".\\\" .\\protos\\adder.proto",
    "grpc-ts-declarations": "yarn run grpc_tools_node_protoc --proto_path=\".\\protos\\\" --plugin=\"protoc-gen-ts=.\\node_modules\\.bin\\protoc-gen-ts.cmd\" --ts_out=\".\\\" .\\protos\\adder.proto",
    "start": "electron ./main.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/shirhatti/studio",
  "license": "MIT",
  "devDependencies": {
    "electron": "^5.0.5",
    "electron-builder": "^20.44.4",
    "grpc-tools": "^1.8.0",
    "typescript": "^3.5.2"
  },
  "build": {
    "appId": "Studio",
    "productName": "Studio",
    "copyright": "Copyright Â© 2019 ${author}",
    "win": {
      "target": "portable"
    }
  },
  "dependencies": {
    "@types/google-protobuf": "^3.7.1",
    "google-protobuf": "^3.8.0",
    "grpc": "^1.21.1"
  }
}
